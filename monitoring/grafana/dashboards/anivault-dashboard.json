{
  "dashboard": {
    "id": null,
    "title": "AniVault Cache-DB Synchronization Dashboard",
    "tags": ["anivault", "cache", "database", "synchronization"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Sync Operations Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "anivault_sync_operations_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(anivault_sync_operations_total{status=\"success\"}[5m]) / rate(anivault_sync_operations_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(anivault_cache_events_total{event_type=\"hit\"}[5m]) / (rate(anivault_cache_events_total{event_type=\"hit\"}[5m]) + rate(anivault_cache_events_total{event_type=\"miss\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Average Operation Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(anivault_sync_operation_duration_seconds_sum[5m]) / rate(anivault_sync_operation_duration_seconds_count[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Sync Operations Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(anivault_sync_operations_total[5m])",
            "refId": "A",
            "legendFormat": "{{operation_type}} - {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Operation Duration Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "anivault_sync_operation_duration_seconds_bucket",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Cache Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(anivault_cache_events_total[5m])",
            "refId": "A",
            "legendFormat": "{{event_type}} - {{operation_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "anivault_cache_size",
            "refId": "A",
            "legendFormat": "Cache Size"
          },
          {
            "expr": "anivault_cache_memory_usage_bytes",
            "refId": "B",
            "legendFormat": "Memory Usage (bytes)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Database Health",
        "type": "stat",
        "targets": [
          {
            "expr": "anivault_database_health_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Unhealthy"
                  },
                  "1": {
                    "text": "Healthy"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "targets": [
          {
            "expr": "anivault_circuit_breaker_state",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Closed"
                  },
                  "1": {
                    "text": "Open"
                  },
                  "2": {
                    "text": "Half-Open"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                },
                {
                  "color": "yellow",
                  "value": 2
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Affected Records",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(anivault_sync_affected_records_total[5m])",
            "refId": "A",
            "legendFormat": "{{operation_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1
  }
}
